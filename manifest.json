{
    "manifest_version": 2,
    "name": "Colorsearch",
    "version": "1.1",

    "permissions": ["tabs"],
  
    "description": "Replaces color searches with color pages ;)",
  
    "icons": {
        "48": "icons/colorsearch-48.png"
    },
  
    "background": {
        "scripts": ["colorsearch.js"],
        "persistent": true
    },

    "browser_action": {
        "default_title": "Colorsearch",
        "default_popup": "popup/settings.html"
    }
}
  